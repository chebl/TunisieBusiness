<label><span style=" float: left; ">{{form_label(form.nomsociete)}}</span>
{#<input class="validate[required]" type="text" name="nom" id="nom" style=" margin-left: 173px; " value=""/>#}
 {{form_widget(form.nomsociete,{'attr': {'class': 'validate[required]','style':'margin-left: 173px;','id':'nom'}})}}  
</label>
<br>
<label><span style=" float: left; ">{{form_label(form.email)}}</span>
{#<input class="validate[required,custom[email]]" type="text" name="email" id="email" style=" margin-left: 164px; " value=""/>#}
{{form_widget(form.email,{'attr': {'class': 'validate[required,custom[email]]','style':'margin-left: 164px;','id':'email','name':'email'}})}} 

</label>
<br>
<label><span style=" float: left; ">{{form_label((form.plainPassword.first))}}</span>
{#<input class="validate[required,length[6,25]]" type="password" name="passwor" id="passwor" style=" margin-left: 115px; "/>#}
{{form_widget(form.plainPassword.first,{'attr': {'class': 'validate[required,length[6,25]]','style':'margin-left: 115px;','id':'passwor'}})}} 

